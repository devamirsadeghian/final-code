//   $('p')       انتخاب <p>‌های موجود در DOM
//   $('<p>')     ساخت یک عنصر جدید با jQuery


var p = document.createElement('p');
var text = p.innerText = 'سلام دنیا';
p.appendChild(text);
document.getElementById("h1").appendChild(p);


var p = $('<p>').text('سلام دنیا');
$('body').append(p);
$('#h1').append(p);



